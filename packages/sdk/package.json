{
    "name": "@mantle-rwa/sdk",
    "version": "0.0.1",
    "description": "SDK for interacting with Mantle RWA Protocol",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
        "lint": "eslint src/",
        "test": "vitest"
    },
    "keywords": [
        "mantle",
        "rwa",
        "sdk",
        "blockchain",
        "web3"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "viem": "^2.0.0"
    },
    "devDependencies": {
        "tsup": "^8.0.0",
        "typescript": "^5.0.0",
        "vitest": "^1.0.0"
    }
}